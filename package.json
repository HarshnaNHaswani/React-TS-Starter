{
  "name": "react-ts-webpack",
  "homepage": "https://harshnanhaswani.github.io/React-TS-Starter/",
  "version": "0.1.0",
  "description": "A Webpack 5 starter kit for React TS",
  "private": true,
  "browserslist": "> 0.25%, not dead",
  "//main": "index.js",
  "analyze": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "build": "webpack --config webpack/webpack.config.js --env env=prod",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint-rule-timings": "cross-env TIMING=1 npm run lint",
    "//lint-rule-timings": "get the time taken to run each rule in eslint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [
    "React",
    "TypeScript",
    "Webpack 5",
    "Starter Template",
    "Boilerplate",
    "EsLint",
    "Prettier",
    "Hot Reload",
    "Responsive Images"
  ],
  "author": "Harshna Haswani",
  "license": "ISC",
  "dependencies": {
    "degit": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "csv-loader": "^3.0.5",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "highlight.js": "^11.6.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "json5": "^2.2.1",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "markdown-loader": "^8.0.0",
    "prettier": "^2.7.1",
    "react-error-boundary": "^3.1.4",
    "react-refresh": "^0.14.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "responsive-loader": "^3.1.0",
    "sharp": "^0.30.7",
    "style-loader": "^3.3.1",
    "toml": "^3.0.0",
    "ts-loader": "^9.3.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "xml-loader": "^1.2.1",
    "yamljs": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
